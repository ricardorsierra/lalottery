{
    "name": "ricardorsierra/lalottery",
    "description": "Add a lottery system to any Laravel apps.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.4.*",
        "laracasts/flash": "~1.0",
        "laracasts/presenter": "0.1.*",
        "donatj/phpuseragentparser": "^0.5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "^0.9.4",
        "orchestra/testbench": "3.4.*|3.4-dev",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Ricardorsierra\\Lalottery\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
